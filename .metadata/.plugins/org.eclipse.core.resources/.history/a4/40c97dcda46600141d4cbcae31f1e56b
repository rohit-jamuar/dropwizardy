package com.dropwizard_application.core;

import com.fasterxml.jackson.annotation.JsonProperty;

public class CipherRepresentation {
	
	private String ciphertext;
	private String plaintext; 
	
	public CipherRepresentation(String ptxt, String ctxt)
	{
		this.plaintext = ptxt;
		this.ciphertext = ctxt;
	}
	
	@JsonProperty
	public String getCTXT()
	{
		return this.ctxt;
	}
}
